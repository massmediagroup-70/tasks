{
  "name": "BlogPageBundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "^4.0",
    "symfony/orm-pack": "^1.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.11"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BlogPageBundle\\": "src"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BlogPageBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
